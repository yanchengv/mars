<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">添加</h3>
  </div>
  <div class="panel-body">
    <%= form_for(@image) do |f| %>
        <%= f.file_field :file %>
        <%= f.submit %>

    <% end %>


    <%= form_for @image, html: { multipart: true } do |f| %>
        <p>
          <label>My Avatar URL:</label>

          <%= f.text_field :url %>
          <%= f.submit %>
        </p>
    <% end %>

  </div>
</div>

